{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@now/node"
    }
  ],
  "functions": {
        "api/server.js": {
          "memory": 1024,
          "includeFiles": "tmp/db.json"
        }
    },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/server.js"
    }
  ]
}
