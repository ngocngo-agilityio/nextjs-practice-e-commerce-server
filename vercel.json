{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "index.js": {
          "memory": 1024,
          "includeFiles": "db.json"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
